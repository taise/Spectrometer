div class='column'
  h1 class=['title', 'is-3'] pg_user list
  p Using 2-5 days of query log history
  table class='table'
    thead
      tr
        th id
        th name
        th count_query
        th last_at
    tbody
      - @users.each do |user|
        tr
          td = user['id']
          td = user['name']
          td = user['count_query']
          td = local_timestamp(user['last_at'])
